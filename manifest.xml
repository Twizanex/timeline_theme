<?xml version="1.0" encoding="UTF-8"?>
<plugin_manifest xmlns="http://www.elgg.org/plugin_manifest/1.8">
	<name>Timeline_theme</name>
	<author>Tom Ondiba</author>
	<version>1.8.15g</version>
	<category>bundled</category>
	<category>content</category>
	<blurb>Timeline_theme plugin</blurb>
	<description>Adds Timeline_theme to your elgg installation site and make it behave just like facebook timeline.</description>
	<website>http://twizanex.com/</website>
	<copyright>Copyright (c) 2011-2014 Thomas.</copyright>
	<license>GNU Public License version 2</license>
	<requires>
		<type>elgg_version</type>
		<version>1.8</version>
	</requires>
	
	<conflicts>
        <type>plugin</type>
        <name>watch_my_pages</name>
        </conflicts>
        
        <conflicts>
        <type>plugin</type>
        <name>custom_index</name>
        </conflicts>
        
        <conflicts>
        <type>plugin</type>
        <name>independence</name>
        </conflicts>
	
	<admin_interface>simple</admin_interface>
	<activate_on_install>true</activate_on_install>
</plugin_manifest>
